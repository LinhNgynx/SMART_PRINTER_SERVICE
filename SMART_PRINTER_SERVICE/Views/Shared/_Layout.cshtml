

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SMART_PRINTER_SERVICE</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SMART_PRINTER_SERVICE.styles.css" asp-append-version="true" />
</head>
<body>
    
    <div class="row gx-0">
        <div class="col-3 gx-0">
       
        </div>
        <div class="col-9">
            <header>
                <div class="title-header">
                    <div class="title-header-content">
                        <image> 

                        </image>
                        Ho Chi Minh City University of Technology
                    </div>
                    <div class="title-header-logout">
                
                    </div>
                </div>
            </header>
            <div class="container  mt-header">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>
       
           
       
        
            

       
 
    
    
       
    
    
    

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - SMART_PRINTER_SERVICE - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
